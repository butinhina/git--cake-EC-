<%= link_to "商品一覧", admin_items_path %>
<%= link_to "ログアウト", destroy_admin_session_path, method: :delete %></br>
<%= link_to "会員一覧", admin_customers_path %></br>
<%= link_to "ジャンル一覧", admin_genres_path %></br>
 <h2>注文履歴一覧</h2>
 <table class="table table-hover table-inverse">
    <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
    </thead>
    <tbody>
      <% @orders.each do |order| %>
        <tr>
          <td><%= order.created_at %></td>
          <td><%= order.customer.first_name %><%= order.customer.last_name %></td>
          <td><%= order.order_details.amount %></td>
          <td></td>
        </tr>
      <% end %>
    </tbody>
 </table>
